{
  "index_patterns": ["avm-filebeat-apache_access_log*"],
  "order" : 99,
  "settings": {
    "number_of_shards": 3
  },
    "mappings" : {
      "doc" : {
        "properties" : {
          "@timestamp" : {
            "type" : "date"
          },
          "@version" : {
                "type" : "keyword",
                "ignore_above" : 256
          },
          "beat" : {
            "properties" : {
              "hostname" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              },
              "name" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              },
              "version" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              }
            }
          },
          "fields" : {
            "properties" : {
              "service_type" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              },
              "topic" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              }
            }
          },
          "host" : {
            "properties" : {
              "name" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              }
            }
          },
          "http" : {
            "properties" : {
              "method" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              },
              "referrer" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              },
              "response" : {
