            "properties" : {
              "build" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              },
              "device" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              },
              "major" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              },
              "minor" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              },
              "name" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              },
              "original" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              },
              "os" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              },
              "os_name" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              }
            }
          }
        }
