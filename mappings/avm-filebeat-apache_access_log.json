                  "body_sent" : {
                    "properties" : {
                      "bytes" : {
                           "type" : "interger"
                      }
                    }
                  },
                  "code" : {
                        "type" : "keyword"
                  }
                }
              },
              "url" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              },
              "version" : {
                    "type" : "keyword"
              }
            }
          },
          "input" : {
            "properties" : {
              "type" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              }
            }
          },
          "offset" : {
            "type" : "long"
          },
          "prospector" : {
            "properties" : {
              "type" : {
                    "type" : "keyword",
                    "ignore_above" : 256
              }
            }
          },
          "read_timestamp" : {
            "type" : "date"
          },
          "source" : {
            "properties" : {
              "ip" : {
